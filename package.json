{
  "name": "nonoto",
  "version": "0.1.1",
  "description": "Electron and web application for interactive music composition",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./app/main.js",
    "link-osmd": "yarn link opensheetmusicdisplay",
    "build:web": "webpack --config webpack.web.js",
    "serve:web": "http-server ./web/dist",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "yarn link-osmd && electron-webpack dev",
    "compile": "yarn link-osmd && electron-webpack"
